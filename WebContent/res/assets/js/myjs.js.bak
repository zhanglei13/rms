    var 7Cell = newRow.insertCell(6);
    var 8Cell = newRow.insertCell(7);
    var 9Cell = newRow.insertCell(8);
    var 10Cell = newRow.insertCell(9);
    var 11Cell = newRow.insertCell(10);
    1Cell.className = "bgc_tt short"; 
    2Cell.className = "bgc_tt short";
    1Cell.innerHTML = "AA";
    2Cell.innerHTML="BB";
    3Cell.innerHTML="SIT";
  
    
}